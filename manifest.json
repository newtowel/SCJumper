{
    "manifest_version": 3,
    "name": "SCJumper",
    "description": "右クリックで現れるコンテキストメニューからサクラチェッカーを利用できます。",
    "version": "1.1",
    "background" : {
      "scripts" : ["background.js"]
    },
    "permissions" : [
      "contextMenus"
    ],
